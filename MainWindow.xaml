<Window x:Class="WPFParsing.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFParsing"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dataGrid" Margin="0,230,0,0"/>
        <Button x:Name="LoadButton" Content="Загрузить данные" HorizontalAlignment="Left" Margin="50,50,0,0" VerticalAlignment="Top" Height="30" Width="137" Click="LoadButton_Click"/>
        <Button x:Name="button" Content="Обновить данные" HorizontalAlignment="Left" Margin="300,50,0,0" VerticalAlignment="Top" Height="30" Width="137" Click="button_Click"/>
        <Button x:Name="LoadNewButton" Content="Загрузить данные с новым ГУИД" HorizontalAlignment="Left" Margin="50,106,0,0" VerticalAlignment="Top" Height="30" Width="137" Click="LoadNewButton_Click"/>
    </Grid>
</Window>
